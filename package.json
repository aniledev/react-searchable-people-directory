{
  "name": "react",
  "version": "1.0.0",
  "eslintConfig": {
		"env": {
			"es2022": true
		},
		"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
		},
		"plugins": [
			"unicorn"
		],
		"rules": {
			"unicorn/better-regex": "error",
			"unicorn/â€¦": "error",
      "unicorn/prevent-abbreviations": [
        "warning",
        {
          "replacements": {
            "e": {
              "event": false
            },
            "props": false,
            "cmd": {
              "command": true
            },
            "errCb": {
              "handleError": true
            }
          }
        }
      ]
    }
	},
  "xo": {
    "import/extensions": [
      "error",
      "never",
      {
        "tsx": "always",
        "js": "always"
      }
    ],
    "semicolon": true,
    "unicornfilename-case": [
      "warning",
      {
        "cases": {
          "camelCase": true,
          "pascalCase": true,
          "kebabCase": false
        },
        "ignore": [
          "src/pages/**/*.tsx"
        ]
      }
    ]
  },
  "homepage": "https://aniledev.github.io/react-searchable-people-directory/",
  "description": "React example starter project",
  "keywords": [
    "react",
    "starter"
  ],
  "main": "src/index.js",
  "dependencies": {
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "phone": "3.1.32",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "^5.0.1",
    "styled-components": "5.3.6",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.13.8",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "gh-pages": "^4.0.0",
    "typescript": "^4.1.3",
    "xo": "^0.53.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "xo && react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "predeploy": "npm run build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
